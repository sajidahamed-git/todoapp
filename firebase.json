{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/app{,/**}",
        "destination": "/index.html"
      }
      ,
    {
      "source": "/app/main.js",
      "destination": "/main.js"
    }
    ]
  }
}
